select CURDATE();
+------------+
| CURDATE()  |
+------------+
| 2022-03-27 |
+------------+

select DATE('2021-01-23 10:12:12');
+-----------------------------+
| DATE('2021-01-23 10:12:12') |
+-----------------------------+
| 2021-01-23                  |
+-----------------------------+

select DATE_ADD('2021-01-12 09:12:23',INTERVAL 20 MINUTE);
+----------------------------------------------------+
| DATE_ADD('2021-01-12 09:12:23',INTERVAL 20 MINUTE) |
+----------------------------------------------------+
| 2021-01-12 09:32:23                                |
+----------------------------------------------------+

select DATE_FORMAT('2021-01-19','%W %M %Y');
+--------------------------------------+
| DATE_FORMAT('2021-01-19','%W %M %Y') |
+--------------------------------------+
| Tuesday January 2021                 |
+--------------------------------------+

 select DATE_FORMAT('2021-01-19','%j %M %Y');
+--------------------------------------+
| DATE_FORMAT('2021-01-19','%j %M %Y') |
+--------------------------------------+
| 019 January 2021                     |
+--------------------------------------+

select DATE_SUB('2021-01-19 07:12:23',INTERVAL 1 HOUR);
+-------------------------------------------------+
| DATE_SUB('2021-01-19 07:12:23',INTERVAL 1 HOUR) |
+-------------------------------------------------+
| 2021-01-19 06:12:23                             |
+-------------------------------------------------+

select DATEDIFF('2021-01-30','2021-01-03');
+-------------------------------------+
| DATEDIFF('2021-01-30','2021-01-03') |
+-------------------------------------+
|                                  27 |
+-------------------------------------+

select EXTRACT(YEAR FROM '2021-01-28');
+---------------------------------+
| EXTRACT(YEAR FROM '2021-01-28') |
+---------------------------------+
|                            2021 |
+---------------------------------+

select GET_FORMAT(DATE,'ISO');
+------------------------+
| GET_FORMAT(DATE,'ISO') |
+------------------------+
| %Y-%m-%d               |
+------------------------+

select DATE_FORMAT('2021-01-28',GET_FORMAT(DATE,'EUR'));
+--------------------------------------------------+
| DATE_FORMAT('2021-01-28',GET_FORMAT(DATE,'EUR')) |
+--------------------------------------------------+
| 28.01.2021                                       |
+--------------------------------------------------+

